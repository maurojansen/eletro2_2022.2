programa
{
	
	funcao inicio()
	{
		real n
		escreva("Algoritmo p/ descobrir a faixa na qual entra um número\n")
		
		escreva("Digite um número: ")
		leia(n)

		se(n <= 10)
		{ 
			escreva("F1 - Faixa 1")
		}
		senao se(n<=100)
		{ 
			escreva("F2 - Faixa 2")
		}
		senao 
		{
			escreva("F3 - Faixa 3")
		}
		
	}
}

